{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint .",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "test:phase1": "node test-phase1.js",
    "test:phase1:ui": "jest __tests__/unit/components/ui --verbose",
    "test:phase1:validation": "jest __tests__/unit/lib/validation.test.ts --verbose",
    "test:phase1:lint": "npm run lint",
    "docker:dev": "docker compose up --build",
    "docker:dev:detached": "docker compose up --build -d",
    "docker:dev:down": "docker compose down",
    "docker:dev:logs": "docker compose logs -f smart-dashboard-dev",
    "docker:prod:build": "docker build -t smart-dashboard:latest --target production .",
    "docker:prod": "docker compose -f docker-compose.prod.yml up --build",
    "docker:prod:detached": "docker compose -f docker-compose.prod.yml up --build -d",
    "docker:prod:down": "docker compose -f docker-compose.prod.yml down",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "docker:rebuild": "docker compose down && docker compose build --no-cache && docker compose up"
  },
